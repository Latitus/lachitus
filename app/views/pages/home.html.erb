<section id="lachitus-slide" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#lachitus-slide" data-slide-to="0" class="active"></li>
    <li data-target="#lachitus-slide" data-slide-to="1"></li>
    <li data-target="#lachitus-slide" data-slide-to="2"></li>
    <li data-target="#lachitus-slide" data-slide-to="3"></li>
    <li data-target="#lachitus-slide" data-slide-to="4"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox">
    <div class="item active">
      <%= image_tag("lachitus-slide1.jpg") %>
    </div>
    <div class="item">
      <%= image_tag("lachitus-slide2.jpg") %>
    </div>
    <div class="item">
      <%= image_tag("lachitus-slide3.jpg") %>
    </div>
    <div class="item">
      <%= image_tag("lachitus-slide4.jpg") %>
    </div>
    <div class="item">
      <%= image_tag("lachitus-slide5.jpg") %>
    </div>
  </div>
</section> <!--/.slider -->

